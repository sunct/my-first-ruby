<h1>New Myblog</h1>


<%= form_for(@myblog) do |f| %>
      <%= f.label :title %>
      <%= f.text_field :title %>

      <%= f.label :user %>
      <%= f.text_field :user_id %>

      <%= f.label :abstract %>
      <%= f.text_field :abstract %>


      <%= f.label :content %>
      <%= f.kindeditor  :content %>



      <%= f.submit "Create my account", class: "btn btn-primary" %>
    <% end %>

<%= link_to 'Back', myblogs_path %>
